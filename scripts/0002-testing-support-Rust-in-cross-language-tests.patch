     'streaming_aead': ['cc', 'go', 'java', 'python'],
     'hybrid': ['cc', 'go', 'java', 'python'],
-    'mac': ['cc', 'go', 'java', 'python'],
+    'mac': ['cc', 'go', 'java', 'python', 'rust'],
     'signature': ['cc', 'go', 'java', 'python'],
-    'prf': ['cc', 'java', 'go', 'python'],
+    'prf': ['cc', 'java', 'go', 'python', 'rust'],
 }
 
 
